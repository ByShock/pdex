{
    "plugins": [
        "react-hooks"
    ],
    "extends": "standard",
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "rules": {
        "semi": 2,
        "react-hooks/rules-of-hooks": "error"
    }
}
